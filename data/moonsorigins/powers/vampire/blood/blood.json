      "type": "origins:execute_command",
      "command": "playsound minecraft:entity.arrow.hit_player master @s ~ ~ ~ 0.25 1"
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
